@model Jeeb.Gateway.Demo.Models.CallBackViewModel
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>CallBack</title>
</head>
<body>
<div>
    @if (Model.IsOk)
    {
        <p>done! waiting for confirmation</p>
        <p>Referece no: @Model.ReferenceNo</p>
        <p>Order no: @Model.OrderNo</p>
    }
